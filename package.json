{
  "name": "game-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && next build",
    "build:static": "cross-env GITHUB_PAGES=true REPO_NAME=game-store next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npx prisma generate",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "db:seed": "npx tsx scripts/seed.ts",
    "deploy": "npm run build && wrangler pages deploy .next",
    "deploy:github": "npm run build:static && npm run deploy:github:pages",
    "deploy:github:pages": "gh-pages -d out",
    "preview": "wrangler pages dev .next"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.5.0",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.15",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.427.0",
    "next": "^14.2.18",
    "next-auth": "^5.0.0-beta.25",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "stripe": "^17.2.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.15.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.1.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.3",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.47",
    "prisma": "^7.2.0",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3"
  }
}
